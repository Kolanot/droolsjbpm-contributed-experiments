{
	"name": "Average Grade",
	"types": [
		{
			"name": "Average",
			"fields": [
				{
					"name": "average",
					"type": "number"
				}
			]
		}
	],
	"output": "Average",
	"rules": [
		{
			"when": [
				"biology >= 50",
				"physics >= 50",
				"math >= 50"
			],
			"then": {
				"output": {
					"average": "($math + $physics + $biology) / 3"
				}
			}
		}
	]
}