{
	"name": "Grade letters",
	"inputs": [
		{
			"name": "grade",
			"type": "number"
		}
	],
	"output": "string",
	"rules": [
		{
			"when": [
				"grade >= 90"
			],
			"then": {
				"output": "A"
			}
		},
		{
			"when": [
				"grade >= 80",
				"grade < 90"
			],
			"then": {
				"output": "B"
			}
		},
		{
			"when": [
				"grade >= 70",
				"grade < 80"
			],
			"then": {
				"output": "C"
			}
		},
		{
			"when": [
				"grade >= 60",
				"grade < 70"
			],
			"then": {
				"output": "D"
			}
		},
		{
			"when": [
				"grade < 60"
			],
			"then": {
				"output": "F"
			}
		}
	]
}
